{
    "name": "webiny/storage",
    "type": "library",
    "description": "Webiny Storage Component",
    "keywords": [
        "storage",
        "amazon s3",
        "local file storage"
    ],
    "homepage": "http://www.webiny.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Al Hamad",
            "email": "sven@webiny.com"
        },
        {
            "name": "Pavel Denisjuk",
            "email": "pavel@webiny.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "webiny/std-lib": "~1.4",
        "webiny/config": "~1.4",
        "webiny/service-manager": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mybuilder/phpunit-accelerator": "dev-master",
        "webiny/amazon": "~1.4"
    },
    "suggest": {
        "webiny/amazon": "Allows you to store files on Amazon AWS S3"
    },
    "autoload": {
        "psr-4": {
            "Webiny\\Component\\Storage\\": ""
        }
    },
    "extras": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}